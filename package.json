{
  "name": "@tdreyno/pretty-please",
  "version": "1.1.1",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "git://github.com/tdreyno/pretty-please.git",
  "scripts": {
    "lint": "tslint --project .",
    "build": "pika build && yarn compress",
    "compress": "terser pkg/dist-web/index.js -o pkg/dist-web/index.min.js --source-map",
    "test": "jest",
    "docs": "typedoc --out docs src",
    "version": "yarn run build",
    "coverage": "jest --coverage --coverageDirectory '../coverage'"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "12.13.1",
    "yarn": "1.19.2"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.16",
    "husky": "^3.1.0",
    "jest": "^24.5.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "terser": "^4.4.2",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "typedoc": "^0.15.4",
    "typescript": "^3.7.3",
    "ts-toolbelt": "^4.12.13"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "peerDependencies": {
    "react": "^16.11.0"
  }
}
